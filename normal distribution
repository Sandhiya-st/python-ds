import numpy as np
import matplotlib.pyplot as plt
arr = np.random.randn(1000)
mean = np.mean(arr)
median = np.median(arr)
std = np.std(arr)
var = np.var(arr)
print(f"Mean: {mean}, Median: {median}, Std: {std}, Variance: {var}")
plt.hist(arr, bins=30, color='skyblue')
plt.title("Histogram of Random Data")
plt.show()

